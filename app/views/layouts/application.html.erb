<!DOCTYPE html>
<html>
<head>
  <title><%= page_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
	<%= render 'layouts/bootstrap_viewport' %>
	<%= render 'layouts/shim' %>
	<meta name="google-site-verification" content="tcchNZXEhaL4jXmYeRtPwEWQWRpUd44xNV_il7Lo8ZE" />
	<!--<link rel="stylesheet" href="/app/assets/stylesheets/font-awesome-4.7.0/css/font-awesome.min.css">-->

<!--	<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>-->

</head>
<body>
  <div id="wrap">
    <%= render 'layouts/header' unless @no_header%>
    <%= render 'layouts/notice' %>
    <%= render 'layouts/alert' %>
    <%= yield %>
    <%#= render 'layouts/footer' unless @no_footer %>
  </div>
  <%#= debug(params) if Rails.env.development? %>
</body>
</html>
